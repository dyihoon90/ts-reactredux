{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"semicolon": [
			"always"
		],
		"indent": [
			true,
			"tabs"
		],
		"variable-name": [
			"ban-keywords",
			"check-format"
		],
		"ordered-imports": [
			false
		],
		"member-ordering": [
			false
		],
		"object-literal-sort-keys": false,
		"curly": false,
		"max-line-length": [
			false
		],
		"no-namespace": [
			false
		],
		"no-var-requires": false,
		"no-empty": false,
		"interface-name": [
			false
		],
		"member-access": [
			"check-accessor"
		],
		"no-empty-interface": false
	},
	"rulesDirectory": []
}
